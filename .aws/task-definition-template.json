{
  "family": "es-iap-be",
  "containerDefinitions": [
    {
      "name": "${CONTAINER_NAME}",
      "cpu": 0,
      "portMappings": [
        {
          "name": "80",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "JASYPT_ENCRYPTOR_PASSWORD",
          "value": "${JASYPT_ENCRYPTOR_PASSWORD}"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "systemControls": []
    }
  ],
  "taskRoleArn": "${EXECUTION_ROLE_ARN}",
  "executionRoleArn": "${EXECUTION_ROLE_ARN}",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  }
}